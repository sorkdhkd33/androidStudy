<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <ImageView
        android:id="@+id/Modifyrect"
        android:layout_width="370dp"
        android:layout_height="500dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/modifyrect" />

    <ImageView
        android:id="@+id/Back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintTop_toTopOf="@+id/Modifyrect"
        app:srcCompat="@drawable/clear" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="75dp"
        android:layout_height="75dp"

        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/Modifypen"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:srcCompat="@drawable/circle_1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:layout_constraintBottom_toBottomOf="@+id/Modifypen"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:srcCompat="@drawable/circledot" />

    <ImageView
        android:id="@+id/Modifypen"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="132dp"
        android:layout_marginRight="132dp"
        app:layout_constraintBottom_toBottomOf="@+id/Modifyrect"
        app:layout_constraintEnd_toEndOf="@+id/Modifyrect"
        app:layout_constraintTop_toTopOf="@+id/Modifyrect"
        app:layout_constraintVertical_bias="0.326"
        app:srcCompat="@drawable/modify" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="76dp"
        android:text="ID :"
        android:textColor="#322878
"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/Modifyrect"
        app:layout_constraintTop_toBottomOf="@+id/Modifypen" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="ID :"
        android:textColor="#322878
"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Modifyrect"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.062" />

</androidx.constraintlayout.widget.ConstraintLayout>